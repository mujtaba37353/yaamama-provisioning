{
  "name": "yamama-middleware-orchestration",
  "version": "0.1.0",
  "description": "Factory service for provisioning WordPress/WooCommerce stores via warm pool",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "dev": "node --watch src/index.js",
    "dev:worker": "node --watch src/worker/worker.js",
    "start": "node src/index.js",
    "start:worker": "node src/worker/worker.js",
    "migrate": "knex --knexfile src/db/knexfile.js migrate:latest",
    "migrate:rollback": "knex --knexfile src/db/knexfile.js migrate:rollback",
    "seed": "knex --knexfile src/db/knexfile.js seed:run",
    "db:reset": "npm run migrate:rollback && npm run migrate && npm run seed"
  },
  "dependencies": {
    "better-sqlite3": "^12.6.2",
    "bullmq": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "express": "^4.18.0",
    "ioredis": "^5.3.0",
    "knex": "^3.1.0",
    "node-ssh": "^13.2.0",
    "pg": "^8.13.0",
    "pino": "^9.0.0",
    "pino-pretty": "^11.0.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.0"
  }
}
